{
  "name": "esb",
  "version": "0.0.5",
  "description": "ESB module",
  "main": "./src/index.ts",
  "repository": "https://glab.lad24.ru/lad/unoto",
  "config": {
    "registry": "rlab.lad24.ru/lad/unoto/unoto-esb"
  },
  "scripts": {
    "test": "rm -rf ./coverage && rm -rf ./.nyc_output && NODE_PATH=../../../:./node_modules nyc cucumber-js --require-module dotenv/config --require-module ts-node/register --require 'features/**/*.ts'",
    "testbug": "NODE_PATH=../../../:./node_modules cucumber-js --require-module dotenv/config --require-module ts-node/register --require 'features/**/*.ts' --tags @bug",
    "testnew": "NODE_PATH=../../../:./node_modules cucumber-js --require-module dotenv/config --require-module ts-node/register --require 'features/**/*.ts' --tags @new",
    "testnewcims": "NODE_PATH=../../../:./node_modules cucumber-js --require-module dotenv/config --require-module ts-node/register --require 'features/**/*.ts' --tags @newcims",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "buildFront": "cd ./frontend/settings/ && npm run build && cd ../../",
    "buildFronta": "clear",
    "start": "npm run lint && npm run format && clear && NODE_PATH=../../../ ts-node -r dotenv/config ./src/index.ts",
    "startf": "npm run buildFronta && npm run lint && npm run format && clear && NODE_PATH=../../../ ts-node -r dotenv/config ./src/index.ts",
    "build": "npm run buildFronta && NODE_PATH=../../../ ./node_modules/.bin/tsc",
    "buildContainer": "MY_KEY=$(cat ~/.ssh/id_rsa) && semver -i $(cat version.num) > version.num && docker build --build-arg SSH_KEY=\"$MY_KEY\" -t $npm_package_config_registry:$(cat version.num) .",
    "pushContainer": "docker push $npm_package_config_registry:$(cat version.num)",
    "deploy": "npm run buildContainer && npm run pushContainer",
    "comp-ts": "tsc -w front/ts/*.ts"
  },
  "author": "Vladislav Selemanov",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/boom": "^7.2.1",
    "@types/chai": "^4.1.7",
    "@types/continuation-local-storage": "^3.2.1",
    "@types/cucumber": "^4.0.6",
    "@types/form-data": "^2.2.1",
    "@types/hapi": "^18.0.2",
    "@types/hapi-auth-jwt2": "^8.6.1",
    "@types/jest": "^23.3.13",
    "@types/joi": "^13.6.3",
    "@types/json2csv": "^4.5.0",
    "@types/node": "^10.14.10",
    "@types/sequelize": "^4.28.3",
    "@types/umzug": "^2.2.2",
    "@types/uuid": "^3.4.4",
    "chai": "^4.2.0",
    "cucumber": "^5.1.0",
    "dotenv": "^6.2.0",
    "mocha": "^6.1.4",
    "nyc": "^13.3.0",
    "prettier": "^1.18.2",
    "semver": "^5.7.0",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@promster/hapi": "^3.1.0",
    "ajv": "^6.10.0",
    "axios": "^0.18.1",
    "bcrypt": "^3.0.6",
    "boom": "^7.3.0",
    "cls-hooked": "^4.2.2",
    "continuation-local-storage": "^3.2.1",
    "enjoi": "^5.0.2",
    "form-data": "^2.4.0",
    "handlebars": "^4.1.2",
    "hapi": "^18.1.0",
    "hapi-auth-bearer-token": "^6.2.1",
    "hapi-swagger": "^9.4.2",
    "inert": "^5.1.3",
    "json2csv": "^4.5.2",
    "jsonschema": "^1.2.4",
    "jsonwebtoken": "^8.5.1",
    "moleculer": "^0.13.9",
    "moment": "^2.24.0",
    "mongoose": "^5.6.1",
    "openstack-swift-client": "^2.1.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "pino": "^5.12.6",
    "pino-pretty": "^2.6.1",
    "prom-client": "^11.5.3",
    "sequelize": "^4.44.0",
    "sequelize-access": "git+ssh://git@glab.lad24.ru:2222/tools/sequelize-access.git",
    "sequelize-hierarchy": "^1.3.2",
    "sequelize-hierarchy-fix": "^1.3.1",
    "stream-to-promise": "^2.2.0",
    "umzug": "^2.2.0",
    "umzug-sync": "^1.0.0",
    "uuid": "^3.3.2",
    "vision": "^5.4.4",
    "xlsx": "^0.14.3",
    "xlsx-style": "^0.8.13"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "typings",
      "src/**/constants/**/*.ts",
      "src/**/interfaces/**/*.ts",
      "src/**/docs/**/*.ts",
      "src/**/routes/**/*.ts",
      "src/**/database/**/*.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
